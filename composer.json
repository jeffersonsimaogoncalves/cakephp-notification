{
  "name": "jeffersonsimaogoncalves/cakephp-notification",
  "description": "CakePHP Notification Plugin provide an notification system",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "plugin",
    "notification"
  ],
  "homepage": "https://github.com/jeffersonsimaogoncalves/cakephp-notification",
  "authors": [
    {
      "name": "Aurélien Schelcher",
      "homepage": "http://www.aschelch.fr",
      "role": "Author"
    },
    {
      "name": "Jefferson Simão Gonçalves",
      "homepage": "https://github.com/jeffersonsimaogoncalves",
      "role": "Author"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "@stable",
    "cakedc/users": "@stable",
    "cakephp/cakephp": "^3.7",
    "cakephp/migrations": "2.0.0"
  },
  "support": {
    "issues": "https://github.com/jeffersonsimaogoncalves/cakephp-notification/issues",
    "source": "https://github.com/jeffersonsimaogoncalves/cakephp-notification"
  },
  "autoload": {
    "psr-4": {
      "JeffersonSimaoGoncalves\\Notification\\": "src"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "htaccess-protect": true,
    "optimize-autoloader": true
  }
}
